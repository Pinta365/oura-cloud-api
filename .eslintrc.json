{
    "rules": {
        "indent": [
            "error",
            4
        ],
        "quotes": [
            2,
            "double"
        ],
        "semi": [
            2,
            "always"
        ],
        "linebreak-style": 0,
        "no-var": 1
    },
    "env": {
        "es2022": true,
        "node": true,
        "browser": true
    },
    "extends": "eslint:recommended"
}